		<AllowUnsafe>true</AllowUnsafe>
                <DebugInformation>false</DebugInformation>
            </Options>
        </Configuration>
        <Files>
            <File>Tao.OpenGl.ExtensionLoader.snk</File>
            <File>Tao.OpenGl.ExtensionLoader.dll.config</File>
        </Files>
	<Project name="Tao.OpenGl.ExtensionLoader" path="." language="C#" type="Library">
	    <Reference name="System" />
            <Files>
                <File>AssemblyInfo.cs</File>
                <File>GlExtensionLoader.cs</File>
            </Files>
        </Project>
    </Solution>
</DNPreBuild>
