<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.6.xsd" version="1.6">
    <Solution name="Tao.GlPostProcess">
        <Configuration name="Debug">
            <Options>
                <CompilerDefines>DEBUG;TRACE;WIN32</CompilerDefines>
                <OptimizeCode>false</OptimizeCode>
                <OutputPath>bin\Debug</OutputPath>
                <XmlDocFile>Tao.GlPostProcess.xml</XmlDocFile>
                <DebugInformation>true</DebugInformation>
            </Options>
        </Configuration>
        <Configuration name="Release">
            <Options>
                <CompilerDefines>TRACE;WIN32</CompilerDefines>
                <OptimizeCode>true</OptimizeCode>
                <OutputPath>bin\Release</OutputPath>
                <XmlDocFile>Tao.GlPostProcess.xml</XmlDocFile>
                <DebugInformation>false</DebugInformation>
            </Options>
        </Configuration>
        <Files>
            <File>Tao.GlPostProcess.License.txt</File>
            <File>Tao.GlPostProcess.Readme.txt</File>
            <File>Tao.GlPostProcess.snk</File>
        </Files>
	<Project name="Tao.GlPostProcess" path="." language="C#" type="Exe">
	    <Reference name="System" />
            <Files>
                <File>AssemblyInfo.cs</File>
                <File>GlPostProcess.cs</File>
            </Files>
        </Project>
    </Solution>
</Prebuild>
