<?xml version="1.0" encoding="utf-8" ?>
<Prebuild xmlns="http://dnpb.sourceforge.net/schemas/prebuild-1.6.xsd" version="1.6">
    <Solution name="Tao.Lua">
        <Configuration name="Debug">
            <Options>
                <CompilerDefines>DEBUG;TRACE;WIN32</CompilerDefines>
                <OptimizeCode>false</OptimizeCode>
                <OutputPath>bin\Debug</OutputPath>
                <XmlDocFile></XmlDocFile>
                <AllowUnsafe>true</AllowUnsafe>
                <DebugInformation>true</DebugInformation>
            </Options>
        </Configuration>
        <Configuration name="Release">
            <Options>
                <CompilerDefines>TRACE;WIN32</CompilerDefines>
                <OptimizeCode>true</OptimizeCode>
                <OutputPath>bin\Release</OutputPath>
                <XmlDocFile>Tao.Lua.xml</XmlDocFile>
                <AllowUnsafe>true</AllowUnsafe>
                <DebugInformation>false</DebugInformation>
            </Options>
        </Configuration>
        <Files>
            <File>Tao.Lua.License.txt</File>
            <File>Tao.Lua.Readme.txt</File>
            <File>Tao.Lua.snk</File>
            <File>Tao.Lua.dll.config</File>
            <File>prebuild.xml</File>
        </Files>
	<Project name="Tao.Lua" path="." language="C#" type="Library">
	    <Reference name="System" />
            <Files>
                <File>AssemblyInfo.cs</File>
                <File>Lua.cs</File>
                <File>LuaL.cs</File>
                <File>DelegateCallingConventionCdeclAttribute.cs</File>
            </Files>
	</Project>
	<Project name="LuaExamples.Simple" path="..\\..\\examples\\LuaExamples\\Simple" language="C#" type="Exe" startupObject="LuaExamples.Simple">
	    <Configuration name="Debug">
		<Options>
		    <CompilerDefines>DEBUG;TRACE;WIN32</CompilerDefines>
		    <OptimizeCode>false</OptimizeCode>
		    <AllowUnsafe>false</AllowUnsafe>
		    <OutputPath>bin\Debug</OutputPath>
		    <XmlDocFile>LuaExamples.Simple.xml</XmlDocFile>
		    <DebugInformation>true</DebugInformation>
		</Options>
	    </Configuration>
	    <Configuration name="Release">
		<Options>
		    <CompilerDefines>TRACE;WIN32</CompilerDefines>
		    <OptimizeCode>true</OptimizeCode>
		    <AllowUnsafe>false</AllowUnsafe>
		    <OutputPath>bin\Release</OutputPath>
		    <XmlDocFile>LuaExamples.Simple.xml</XmlDocFile>
		    <DebugInformation>false</DebugInformation>
		</Options>
	    </Configuration>
	    <Reference name="System" /> 
	    <Reference name="Tao.Lua" />
	    <Files>
		<File>AssemblyInfo.cs</File>
		<File>Simple.cs</File>
		<File buildAction="Content">simple.lua</File>
	    </Files>
	</Project>
	<Project name="LuaExamples.Functions" path="..\\..\\examples\\LuaExamples\\Functions" language="C#" type="Exe" startupObject="LuaExamples.Functions">
	    <Configuration name="Debug">
		<Options>
		    <CompilerDefines>DEBUG;TRACE;WIN32</CompilerDefines>
		    <OptimizeCode>false</OptimizeCode>
		    <AllowUnsafe>false</AllowUnsafe>
		    <OutputPath>bin\Debug</OutputPath>
		    <XmlDocFile>LuaExamples.Functions.xml</XmlDocFile>
		    <DebugInformation>true</DebugInformation>
		</Options>
	    </Configuration>
	    <Configuration name="Release">
		<Options>
		    <CompilerDefines>TRACE;WIN32</CompilerDefines>
		    <OptimizeCode>true</OptimizeCode>
		    <AllowUnsafe>false</AllowUnsafe>
		    <OutputPath>bin\Release</OutputPath>
		    <XmlDocFile>LuaExamples.Functions.xml</XmlDocFile>
		    <DebugInformation>false</DebugInformation>
		</Options>
	    </Configuration>
	    <Reference name="System" /> 
	    <Reference name="Tao.Lua" />
	    <Files>
		<File>AssemblyInfo.cs</File>
		<File>Functions.cs</File>
		<File buildAction="Content">functions.lua</File>
	    </Files>
	</Project>
    </Solution>
</Prebuild>
