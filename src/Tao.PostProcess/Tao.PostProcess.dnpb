            <Files>
                <File>AssemblyInfo.cs</File>
                <File>AppMain.cs</File>
                <File>BuildProcessor.cs</File>
                <File>Options.cs</File>
                <File>ReleaseBuildProcessor.cs</File>
                <File>UsageHelp.cs</File>
            </Files>
        </Project>
    </Solution>
</DNPreBuild>
