        </Configuration>
        <Files>
            <File>Tao.PostProcess.License.txt</File>
            <File>Tao.PostProcess.Readme.txt</File>
            <File>Tao.PostProcess.snk</File>
        </Files>
	<Project name="Tao.PostProcess" path="." language="C#" type="Exe">
